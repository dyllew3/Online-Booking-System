<div>
<h1>Approve</h1>

<% if @user.admin? and @user.authenticated?%>
  <%= form_for :approved do |f| %>
	<% not_authenticated.each do |item|  %>	
		<p><%= f.check_box item.email %><%= item.first_name  %>  <%= item.last_name  %>  <%= item.email %><br></p>
	<% end  %>
	<div class="actions">
    <%= f.submit "Save Approved", class:'button' %>
  </div>

	<%end%>
<%end%>
<div>
